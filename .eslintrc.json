{
    "env": {
        "es6": true
    },
    "extends": [
        "standard",
        "plugin:react/recommended"
    ],
    "globals": {
        "document": true,
        "window": true,
        "console": true,
        "$$": true,
        "wt1": true,
        "LazyLoad": true,
        "URLSearchParams": true
    },
    "parserOptions": {
        "ecmaVersion": 6
    },
    "parser": "babel-eslint",
    "rules": {
        "comma-dangle": 0,
        "indent": [
            "error", 4, {
                "SwitchCase": 1
            }
        ]
    }
}
